{
  "description": "Source definitions with type, schedule, and rate limits. Matches FinalStrategy.md execution schedule.",
  "sources": {
    "greenhouse": {
      "type": "direct-api",
      "phase": 1,
      "enabled": true,
      "schedule": "0 */3 * * *",
      "scheduleDescription": "Every 3 hours",
      "endpointTemplate": "https://boards-api.greenhouse.io/v1/boards/{company}/jobs",
      "rateLimiting": {
        "delayBetweenRequestsMs": 200,
        "batchSize": 20,
        "batchPauseMs": 2000,
        "maxRetries": 3,
        "backoffStartMs": 5000
      },
      "timeoutMs": 30000
    },
